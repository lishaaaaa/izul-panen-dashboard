{
  "functions": {
    "api/index.py": {
      "runtime": "@vercel/python@4.0.0",
      "includeFiles": "{templates/**,static/**,app.py,sheets_io.py}"
    },
    "api/ping.py": {
      "runtime": "@vercel/python@4.0.0"
    }
  },
  "routes": [
    { "src": "^/api/ping$", "dest": "/api/ping.py" },
    { "src": "^(?!/api/).*", "dest": "/api/index.py" }
  ]
}
